<UserControl x:Class="CM79D3_GUI_2023242.WpfClient.Views.EmergencyCallView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CM79D3_GUI_2023242.WpfClient.Views"
             xmlns:vms="clr-namespace:CM79D3_GUI_2023242.WpfClient.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <DataGrid Grid.Column="0">
            <DataGrid.Columns>
                <DataGridTemplateColumn Header="ID"></DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Caller's Name"></DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Caller's Phone"></DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Location"></DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Incidents' Type"></DataGridTemplateColumn>
                <DataGridTemplateColumn Header="Date"></DataGridTemplateColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
